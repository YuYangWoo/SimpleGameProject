<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.simplegameproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameListActivity" />
        <activity android:name=".MyInformationActivity" />
        <activity android:name=".ShareScoreActivity" />
        <activity android:name=".RankingActivity" />
        <activity android:name=".ReactGameMainActivity" />
        <activity android:name=".ReactGameStartActivity" />
        <activity android:name=".ReactGameAgainActivity" />
        <activity android:name=".ReactGameClickActivity" />
        <activity android:name=".ReactGameResultActivity"/>
        <activity android:name=".Remember_Game_MainActivity" />
    </application>

</manifest>